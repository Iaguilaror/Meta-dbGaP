< ../config.mk

all:VQ:
	bin/targets | xargs mk

results/%/metadata.xml:Q:
	##set -x
	echo "[DEBUGGING] downloading $target"
	mkdir -p `dirname $target`
	wget -q -O - $BASE_URL/$stem/ \
	| grep '\.xml' \
	| grep 'GapExchange' \
	| cut -d'"' -f2 \
	| xargs wget -q -O - > $target.build \
	&& mv $target.build $target || echo "[WARNING] No GapExchange .xml file found for $stem"
